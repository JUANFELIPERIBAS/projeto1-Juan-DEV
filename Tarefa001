HorasExtras = float(input("Digite o número de horas extras: "))
HorasFaltas = float(iinput("Digite o número de horas Faltantes: "))

TotalMinutos = (HorasExtas - (2/3*HorasFaltas)) * 60

print(TotalMinutos)